  "license": "ISC",
  "engines": {
    "node": ">=16.6.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "datetime-difference": "^1.0.2",
    "discord-oauth2": "^2.7.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "full-icu": "^1.3.4",
    "got": "^11.8.2",
    "htmlparser2": "^6.1.0",
    "npm": "^7.20.6",
    "pg": "^8.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Markus-Rost/discord-wiki-bot.git"
  },
